@using System.Web.UI.DataVisualization.Charting
@{
var chart = new System.Web.UI.DataVisualization.Charting.Chart();
var formula = chart.DataManipulator.Statistics.NormalDistribution(0);
}
<html>
	<head>
		<meta charset="utf-8">
		<title>Option Pricing</title>
	</head>
	<body style="background-color: #1A00AA">
		<form action="" method="post">
			<p><label for="S0">Цена актива:</label><br>
			<input type="text" name="S0"/></p>

			<p><label for="K">Страйк-цена опциона:</label><br>
			<input type="text" name="K"/></p>
		</form>
		<p>The result is @formula</p>
	</body>
</html>